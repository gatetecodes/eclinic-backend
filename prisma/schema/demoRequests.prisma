model DemoRequest {
    id           Int               @id @default(autoincrement())
    clinic_name  String
    email        String
    phone_number String
    address      String
    demo_date    DateTime
    createdAt    DateTime          @default(now())
    updatedAt    DateTime          @updatedAt
    status       DemoRequestStatus @default(PENDING)
}

enum DemoRequestStatus {
    PENDING
    APPROVED
    REJECTED
}
